# General Configuration
AGENT_CONFIG_FILE="D:\projects\Fetch.ai Hackathon\IntelliRescue-AI-Framework\agent_addresses.json"   # Path to centralized agent addresses file

# AI Agent Configuration
AGENTVERSE_KEY=  # Replace with your AgentVerse API key for AI registration
AI_KEY=your_ai_identity_key_here            # Replace with your AI's identity key for FetchAI interactions
AI_WEBHOOK=https://your_ai_webhook_url_here # Replace with the AI's webhook URL for receiving requests

# External API Keys
MAPS_API_KEY=your_google_maps_api_key_here       # Replace with your Google Maps API Key for navigation
WEATHER_API_KEY=your_weather_api_key_here        # Replace with your Weather API key (e.g., OpenWeatherMap)

# Dashboard Configuration
DASHBOARD_HOST=127.0.0.1             # Host for the React-based dashboard
DASHBOARD_PORT=8000                  # Port for the dashboard

# Database/Storage (Optional if Needed)
DB_HOST=localhost                    # Database host (if using a database for any data storage)
DB_PORT=5432                         # Database port
DB_NAME=your_database_name           # Name of the database
DB_USER=your_database_username       # Database username
DB_PASSWORD=your_database_password   # Database password

# Logging and Miscellaneous Settings
LOG_LEVEL=INFO                       # Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
ENV=development                      # Environment (e.g., development, staging, production)
TIMEZONE=UTC                         # Timezone for logging and timestamp consistency

# Security Settings
JWT_SECRET=your_jwt_secret_key       # Secret key for JWT authentication (if used)
ENCRYPTION_KEY=your_encryption_key   # Encryption key for securing sensitive data
